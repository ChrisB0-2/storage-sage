{
  "title": "StorageSage â€“ Cleanup Metrics",
  "uid": "storagesg1",
  "description": "Real-time cleanup performance and reliability metrics from StorageSage.",
  "refresh": "30s",
  "schemaVersion": 38,
  "version": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
    "time_options": ["5m", "15m", "30m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "panels": [
    {
      "id": 1,
      "title": "Files Deleted / s",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(storagesage_files_deleted_total[5m])",
          "legendFormat": "Files deleted"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "lines",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Bytes Freed / s",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(storagesage_bytes_freed_total[5m])",
          "legendFormat": "Bytes freed"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytesps",
          "custom": {
            "drawStyle": "lines",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Errors (last hour)",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "increase(storagesage_errors_total[1h])"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.0000001 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 9
      }
    },
    {
      "id": 4,
      "title": "Cleanup Duration p95 (seconds)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, rate(storagesage_cleanup_duration_seconds_bucket[5m]))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "custom": {
            "drawStyle": "lines",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 9
      }
    }
  ]
}
